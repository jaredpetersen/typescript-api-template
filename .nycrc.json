{
  "check-coverage": true,
  "per-file": true,
  "lines": 90,
  "statements": 90,
  "functions": 90,
  "branches": 90,
  "extension": [
    ".ts"
  ],
  "exclude": [
    "src/**/*.test-*.ts",
    "src/**/*.d.ts",
    "src/logger.ts"
  ],
  "reporter": [
    "lcov",
    "json",
    "text-summary"
  ],
  "temp-dir": "./coverage/.nyc_output",
  "report-dir": "./coverage"
}