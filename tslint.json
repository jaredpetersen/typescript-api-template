{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}